{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "openGraph": {
      "type": "component",
      "repeatable": false,
      "component": "seo.open-graph",
      "required": true
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "string",
      "unique": false,
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "regex": "^[a-zA-Z0-9-]+$"
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "body": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "header-sections.header-with-cards",
        "service-sections.service-three-column-with-large-icons",
        "solution-sections.solution-three-column-with-large-icons",
        "content-section.content-markdown",
        "contact-sections.contact-split-with-pattern"
      ]
    }
  }
}
