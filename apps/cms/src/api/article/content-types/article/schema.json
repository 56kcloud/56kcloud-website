{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "readTime": {
      "type": "integer",
      "default": 1,
      "required": true
    },
    "publishedOn": {
      "type": "date"
    },
    "slug": {
      "type": "string",
      "required": true,
      "regex": "^[a-zA-Z0-9-]+$"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team-member.team-member"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "relatedPartners": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::partner.partner",
      "inversedBy": "relatedArticles"
    },
    "relatedArticles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "relatedSolutions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::solution.solution"
    }
  }
}
