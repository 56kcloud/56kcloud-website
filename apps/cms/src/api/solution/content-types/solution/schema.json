        "list.company-list",
        "list.feature-list",
        "list.illustration-card-list",
        "list.partner-list",
        "hero.home-hero",
        "text.h1",
        "title-section.medium-title-section",
        "title-section.small-title-section",
        "hero.centered-text-hero",
        "hero.basic-hero",
        "hero.features-hero",
        "hero.hero-with-image-tiles"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
